{
  "name": "bootkit-ng0-app",
  "version": "0.0.0",
  "homepage": "https://bootkitlib.github.io/",
  "author": "BootKit",
  "scripts": {
    "start": "ng serve",
    "build": "ng build ng0",
    "publish": "npm publish ./dist/ng0 --access=public",
    "build-publish": "npm run build && npm run publish",
    "build-link": "npm run build && cd dist/ng0 && npm link",
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "css:bootstrap:build": "sass node_modules/bootstrap/scss/bootstrap.scss public/css/bootstrap.css",
    "css:bootstrap:build-rtl": "npm run css:bootstrap:build & rtlcss public/css/bootstrap.css public/css/bootstrap.rtl.css"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@angular/ssr": "^20.2.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0",
    "@angular/animations": "^20.1.6",
    "@angular/cdk": "^20.2.0",
    "highlight.js": "^11.10.0"
  },
  "devDependencies": {
    "@angular/build": "^20.2.0",
    "@angular/cli": "^20.2.0",
    "@angular/compiler-cli": "^20.2.0",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2",

    "ng-packagr": "^20.2.0",
    "prettier": "3.6.2",
    "rtlcss": "^4.3.0",
    "sass": "^1.89.2",
    "bootstrap": "^5.3.7"
  }
}